<Page x:Class="New_AutoTest.Pages.ExamResultPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:New_AutoTest.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="700" d:DesignWidth="500"
      Title="ExamResultPage">

    <Grid>
        <StackPanel Margin="30">
            <Label Content="Result" FontSize="30" FontWeight="DemiBold" HorizontalAlignment="Center"></Label>
            <Label Content="QuestionCount:" FontSize="20" FontWeight="DemiBold"></Label>
            <TextBlock x:Name="QuestionBlock" FontSize="20" FontWeight="DemiBold"></TextBlock>
            <Label Content="CorrectAnswerCount:" FontSize="20" FontWeight="DemiBold"></Label>
            <TextBlock x:Name="CorrectAnswerBlock" FontSize="20" FontWeight="DemiBold"></TextBlock>
            <Button Content="Menu" Click="MenuButton_Click" FontSize="20" FontWeight="DemiBold" Margin="0,30,0,0" Width="60" Height="40" Background="LightBlue"></Button>
        </StackPanel>
    </Grid>
</Page>
